                                </Grid>
                            </GroupBox>
                            <Button x:Name="btnPowerRefresh" Content="Refresh" HorizontalAlignment="Left" Margin="10,134,0,0" VerticalAlignment="Top" Width="136" Click="btnPowerRefresh_Click"/>
                            <Button x:Name="btnPowerON" Content="ON" HorizontalAlignment="Left" Margin="151,23,0,0" VerticalAlignment="Top" Width="50" Height="41" Click="btnPowerON_Click"/>
